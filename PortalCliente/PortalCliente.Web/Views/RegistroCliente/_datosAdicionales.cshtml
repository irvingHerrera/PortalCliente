				<input type="number" class="form-control datos" data-ng-model="modelo.VentaAnual" placeholder="" id="txtDAVA">
			</div>
		</div>
	</div>

	<div class="row">
		<div class="col-sm-4">
			<div class="form-group">
				<div class="tag-text">Página web</div>
				<input type="text" class="form-control datos" data-ng-model="modelo.PaginaWeb" placeholder="" id="txtPW">
			</div>
		</div>
