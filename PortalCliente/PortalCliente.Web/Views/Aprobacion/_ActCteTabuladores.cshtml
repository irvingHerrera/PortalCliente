<div class="form-sections">

    <div class="row no-margin">
        <h3 class="subtitle">Tabuladores</h3>

        <div class="row">

            <div class="col-sm-6">
                <div class="table-responsive input-edit" style="margin-top: 0;">
                    <table class="table table-striped table-bordered table-hover">
                        <thead class="table-rows star">
                            <tr>
                                <th>Concepto</th>
                                <th>CtePed</th>
                                <th>CeteFact</th>
                                <th>Empresa que factura</th>
                            </tr>
                        </thead>
                        <tbody id="numbers1">
                            <tr class="table-rows" ng-repeat="i in modelActCte.Tabuladores" id="fila{{$index + 1}}">
                                <td><input type="text" class="form-control" data-ng-model="i.Concepto" data-ng-disabled="inputDisable"></td>
                                <td>
                                    <input type="checkbox" id="CtePed{{$index + 1}}" class="form-control" data-ng-checked="i.CtePed" data-ng-disabled="inputDisable">
                                    <label for="CtePed{{$index + 1}}"><span><div></div></span></label>
                                </td>
                                <td>
                                    <input type="checkbox" id="CeteFact{{$index + 1}}" class="form-control" data-ng-checked="i.CeteFact" data-ng-disabled="inputDisable">
                                    <label for="CeteFact{{$index + 1}}"><span><div></div></span></label>
                                </td>
                                <td>
                                    @*<input type="text" class="form-control" data-ng-model="i.Empresa" data-ng-disabled="inputDisable">*@
                                    <select class="form-control" data-ng-model="i.Empresa">
                                        <option value="">Seleccionar</option>
                                        <option value="ELI">ELI</option>
                                        <option value="RPF">RPF</option>
                                        <option value="EILO">EILO</option>
                                    </select>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="col-sm-6">

                <div class="col-sm-12">
                    <div class="tag-text">Cta AME: Para la factura de ELI a la Americana utilizar el TC del:</div>
                    <div class="form-group input-edit">
                        @*<input id="inp3" type="text" data-ng-model="modelActCte.CtaAME" class="form-control" data-ng-disabled="inputDisable" />*@
                        <select class="form-control" data-ng-model="modelActCte.CtaAME"  data-ng-disabled="inputDisable">
                            <option value="">Seleccionar</option>
                            <option value="DOF">DOF</option>
                            <option value="Pedimento">Pedimento</option>
                        </select>
                    </div>
                </div>

                <div class="col-sm-6">
                    <div class="tag-text">Cta AME/expedida a:</div>
                    <div class="form-group input-edit">
                        @*<input id="inp3" type="text" data-ng-model="modelActCte.CtaAMEExpA" class="form-control" data-ng-disabled="inputDisable" />*@
                        <select class="form-control" data-ng-model="modelActCte.CtaAMEExpA" data-ng-disabled="inputDisable">
                            <option value="">Seleccionar</option>
                            <option value="ClientePedimento">Cliente Pedimento</option>
                            <option value="ClienteFactura">Cliente Factura</option>
                        </select>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="tag-text">Cta AME/se cobra en:</div>
                    <div class="form-group input-edit">
                        @*<input id="inp3" type="text" data-ng-model="modelActCte.CtaAMECobra" class="form-control" data-ng-disabled="inputDisable" />*@
                        <select class="form-control" data-ng-model="modelActCte.CtaAMECobra" data-ng-disabled="inputDisable">
                            <option value="">Seleccionar</option>
                            <option value="DirectoUSA">Directo USA</option>
                            <option value="Rendición">Rendición</option>
                        </select>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="tag-text">Compr/Expedidor por:</div>
                    <div class="form-group input-edit">
                        @*<input id="inp3" type="text" data-ng-model="modelActCte.CtaAMEExpPor" class="form-control" data-ng-disabled="inputDisable" />*@
                        <select class="form-control" data-ng-model="modelActCte.CtaAMEExpPor" data-ng-disabled="inputDisable">
                            <option value="">Seleccionar</option>
                            <option value="ClientePedimento">Cliente Pedimento</option>
                            <option value="ClienteFactura">Cliente Factura</option>
                        </select>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group text-center input-edit" style="margin-top: 25px;">
                        <input id="ImpPECA" type="checkbox" data-ng-checked="modelActCte.ImpPECA" class="form-control" data-ng-disabled="inputDisable" />
                        <label for="ImpPECA"><span><div></div></span> Impuestos por PECA</label>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
